{
  "name": "Agentic Web Learning Tool",
  "description": "Tooling for understanding Agentic Web",
  "version": "0.0.0",
  "minimum_chrome_version": "120",
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "incognito": "split",
  "permissions": [
    "storage",
    "tabs",
    "webNavigation",
    "webRequest",
    "cookies",
    "debugger",
    "management",
    "contentSettings",
    "scripting",
    "sidePanel"
  ],
  "host_permissions": ["*://*/*"],
  "devtools_page": "devtools/devtools.html",
  "options_page": "options/options.html",
  "side_panel": {
    "default_path": "sidePanel/sidePanel.html"
  },
  "background": {
    "service_worker": "serviceWorker/serviceWorker.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "icons/icon-48.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "assets/data/*.json",
        "data/*json",
        "report/index.html",
        "prebid-interface.js"
      ],
      "matches": ["*://*/*"]
    }
  ]
}
